{
  "name": "ranganmag",
  "version": "1.0.0",
  "description": "A modern newspaper website with React.js frontend and Node.js backend",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "newspaper",
    "react",
    "nodejs",
    "firebase"
  ],
  "author": "Ranganmag Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "form-data": "^4.0.4"
  }
}
